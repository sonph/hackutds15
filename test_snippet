# basic algebra
var a = 2;
var b = 4;
var c = a * b;
c = a + b;
c ++;
c -= ((a % b) * a / b) + 12;

# find sum of a list
var l = [22, 3, 5, 10, 12];
var s = 0;
for (x in l) {
	s += x;
}
s = 0;
for (var index = 0; index < l.length; index++) {
	s = s + l[index];
}

# function call
var l = 'hello world';
l.slice(2, 6);